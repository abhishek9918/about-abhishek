{
  "name": "front-craft",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "explore-bundle": "source-map-explorer dist/FrontCraft/main-*.js dist/FrontCraft/main-*.js.map --html bundle-analysis.html",
    "analyze": "npm run explore-bundle",
    "analyze-esbuild": "esbuild-visualizer --metadata dist/FrontCraft/stats.json --filename bundle-report.html"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.3",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.7",
    "rxjs": "~7.8.0",
    "swiper": "^11.2.10"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^20.0.0",
    "@angular/cli": "^20.1.3",
    "@types/aos": "^3.0.7",
    "esbuild-visualizer": "^0.7.0",
    "source-map-explorer": "^2.5.3"
  }
}
